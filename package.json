{
  "name": "hydrusrv",
  "version": "7.0.0",
  "description": "A small application for serving media handled with hydrus server via API",
  "keywords": [
    "hydrus",
    "hydrus network",
    "hydrusnetwork",
    "booru",
    "api",
    "express",
    "expressjs",
    "node",
    "nodejs"
  ],
  "author": "Michael Serajnik <ms@mserajnik.at>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://mserajnik@github.com/mserajnik/hydrusrv.git"
  },
  "bugs": {
    "url": "https://github.com/mserajnik/hydrusrv/issues"
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "sources": [
      "**/*.js"
    ],
    "failFast": true,
    "failWithoutAssertions": true,
    "tap": false,
    "compileEnhancements": true
  },
  "scripts": {
    "start": "node ./bin/www",
    "start-dev": "nodemon --ignore './node_modules/' ./bin/www",
    "migrate": "node ./storage/migrate.js",
    "test": "ava --verbose"
  },
  "dependencies": {
    "@phc/argon2": "^1.0.9",
    "better-sqlite3": "^5.4.0",
    "body-parser": "^1.18.3",
    "distributions": "^2.0.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "file-type": "^10.7.1",
    "morgan": "^1.9.1",
    "object-hash": "^1.3.1",
    "read-chunk": "^3.0.0",
    "upash": "^1.0.2"
  },
  "devDependencies": {
    "ava": "1.2.0",
    "eslint": "^5.12.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "fs-extra": "^7.0.1",
    "nodemon": "^1.18.9"
  }
}
